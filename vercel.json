{
  "version": 2,
  "buildCommand": "next build",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["sfo1"],
  "env": {
    "NEXT_PUBLIC_API_URL": "/api",
    "NEXT_PUBLIC_MAX_FILE_SIZE": "10485760",
    "NEXT_PUBLIC_SUPPORTED_FORMATS": "jpeg,jpg,png,webp",
    "NEXT_PUBLIC_MAX_BATCH_SIZE": "10",
    "NEXT_PUBLIC_ENABLE_AI_FEATURES": "true",
    "NODE_ENV": "production"
  }
} 